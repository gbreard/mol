    "patrones_regex": [
      {
        "patron": "(?:sera|seras|debera|deberas)\\s+(?:responsable\\s+de\\s+)?([^.;\\n]{10,100})",
        "descripcion": "Sera responsable de X"
      },
      {
        "patron": "(?:funciones|tareas|responsabilidades)\\s*(?:principales)?\\s*:?\\s*([^.;]{10,200})",
        "descripcion": "Funciones: X"
      },
      {
        "patron": "(?:a\\s+cargo\\s+de|encargarse\\s+de)\\s+([^.;\\n]{10,100})",
        "descripcion": "A cargo de X"
      }
    ],

