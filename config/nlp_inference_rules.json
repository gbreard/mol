    "_descripcion": "Extraccion de tareas cuando LLM no detecta bullets",
    "_gap_actual": "46% ofertas sin tareas",
    "_regla_excel": "üü¢7 - No depender solo de bullet points",

    "patrones_inicio_frase": [
      "sera responsable de",
      "seras responsable de",
      "sus funciones incluyen",
      "tus funciones incluyen",
      "funciones principales",
      "principales funciones",
      "principales tareas",
