  "contexto_empresarial": {
    "_descripcion": "Palabras que indican contexto empresarial (eliminar despues de guion)",
    "palabras": [
      "importante", "empresa", "compania", "grupo", "consultora",
      "gastronomia", "facility", "corporativo", "corporativa", "industria",
      "concesionario", "maquinaria", "automotriz", "multinacional",
      "pyme", "startup", "holding"
    ]
  },

  "parentesis_eliminar": {
    "_descripcion": "Patrones de parentesis a eliminar",
    "patrones": [
      {"patron": "\\s*\\([^)]{15,}\\)$", "ejemplo": "(Consumo Masivo / Grandes Cuentas)", "razon": "parentesis largo al final"},
      {"patron": "\\s*\\(PYME\\)$", "ejemplo": "(PYME)"},
      {"patron": "\\s*\\(para\\s+[^)]+\\)$", "ejemplo": "(para cliente bancario)"},
      {"patron": "\\s*\\(Eventual\\)(\\s+\\w+)?$", "ejemplo": "(Eventual) Moreno"},
      {"patron": "\\s*\\(Corredor\\)$", "ejemplo": "(Corredor)"},
      {"patron": "\\s*\\(Externo/?a?\\)$", "ejemplo": "(Externo/a)"},
      {"patron": "\\s*\\(Temporal\\)$", "ejemplo": "(Temporal)"},
      {"patron": "\\s*\\(Reemplazo\\)$", "ejemplo": "(Reemplazo)"},
      {"patron": "\\s*\\(Part[- ]?time\\)$", "ejemplo": "(Part time)"},
      {"patron": "\\s*\\(Full[- ]?time\\)$", "ejemplo": "(Full time)"},
      {"patron": "\\s*\\([A-Z][a-záéíóú]+(?:\\s+[A-Z]?[a-záéíóú]+)*\\)$", "ejemplo": "(Florida Oeste)", "razon": "ubicacion en parentesis"}
    ]
  },

  "ubicacion_con_guion": {
    "_descripcion": "Ubicaciones precedidas por guion LARGO (– o —) con parentesis opcional",
    "patrones": [
      {"patron": "\\s*[–—]\\s*[A-Z][a-záéíóú]+(?:\\s+[A-Z]?[a-záéíóú]+)*\\s*\\([^)]+\\)$", "ejemplo": "– Vicente López (Florida Oeste)"},
      {"patron": "\\s*[–—]\\s*[A-Z][a-záéíóú]+(?:\\s+[A-Z]?[a-záéíóú]+)*$", "ejemplo": "– Vicente López"}
    ]
  },

  "contexto_ubicacion": {
    "_descripcion": "Patrones de contexto + ubicacion al final (para X en Ciudad)",
    "patrones": [
      {"patron": "\\s+para\\s+\\w+\\s+en\\s+[A-Z][a-záéíóú]+(?:\\s+[A-Z]?[a-záéíóú]+)*$", "ejemplo": "para farmacias en Rio Cuarto"},
      {"patron": "\\s+en\\s+[A-Z][a-záéíóú]+(?:\\s+del?\\s+)?[A-Z]?[a-záéíóú]*$", "ejemplo": "en Mar del Plata"},
      {"patron": "\\s+para\\s+(?:empresa|compania|cliente)\\s+en\\s+\\w+$", "ejemplo": "para empresa en Cordoba"}
    ]
  },

  "multi_perfil": {
    "_descripcion": "Configuracion para detectar y expandir titulos con multiples perfiles",
    "separadores": [", ", " y ", " / ", " o "],
    "min_perfiles": 2,
    "min_largo_perfil": 5,
    "no_ocupacion": [
      "zona", "alrededor", "cerca", "barrio", "caba", "gba", "norte", "sur", "oeste", "este",
      "nuevo", "usados", "okm", "0km", "usado", "nuevos",
      "leer", "ver", "requisitos", "detalles", "importante",
      "turno", "mañana", "tarde", "noche", "full", "part"
    ],
    "_nota": "Un titulo se considera multi-perfil si tiene 2+ items separados por coma que parecen ocupaciones"
  },

  "preposiciones_final": {
    "_descripcion": "Preposiciones que quedan sueltas al final y deben eliminarse",
    "palabras": ["para", "de", "en", "a", "con", "por"]
  },

  "limpieza_final": {
    "_descripcion": "Patrones de limpieza al final del proceso",
    "patrones": [
      {"patron": "\\s+", "reemplazo": " ", "descripcion": "espacios multiples"},
      {"patron": "[\\s\\-/]+$", "reemplazo": "", "descripcion": "guiones/barras sueltos al final"}
    ]
  },

  "codigos_final": {
    "_descripcion": "Codigos numericos al FINAL del titulo (ej: - 1729, - 4521)",
    "_nota": "FASE 1 v2.2 - detectados en revision 100 IDs",
    "patrones": [
      {"patron": "\\s*-\\s*\\d{4}$", "ejemplo": "DevOps Engineer - Remoto - 1729"},
      {"patron": "\\s*-\\s*\\d{3,5}$", "ejemplo": "Analista - 12345"},
      {"patron": "\\s*#\\d{3,5}$", "ejemplo": "Vendedor #1234"},
      {"patron": "\\s*\\(\\d{4,5}\\)$", "ejemplo": "Contador (12345)"}
    ]
  },

  "modalidad_guion": {
    "_descripcion": "Modalidad con guion al final (casos complejos detectados en revision)",
    "_nota": "FASE 1 v2.2 - patron: titulo - modalidad - codigo/ubicacion",
    "patrones": [
      {"patron": "\\s*-\\s*[Rr]emoto\\s*-.*$", "ejemplo": "DevOps - Remoto - 1729"},
      {"patron": "\\s*-\\s*[Oo]n\\s*[Ss]ite\\s*-.*$", "ejemplo": "Java Dev - On Site - CABA"},
      {"patron": "\\s*-\\s*[Mm]ix\\s*\\([^)]+\\).*$", "ejemplo": "Architect - Mix (On Site & Remoto) - Retiro"},
      {"patron": "\\s*-\\s*[Hh][ií]brido\\s*-.*$", "ejemplo": "Analista - Híbrido - Norte"},
      {"patron": "\\s*-\\s*[Pp]resencial\\s*-.*$", "ejemplo": "Contador - Presencial - Centro"},
      {"patron": "\\s*-\\s*(Remoto|On Site|Hibrido|Híbrido|Presencial|Mix)$", "ejemplo": "DevOps - Remoto", "_nota": "modalidad sola al final"}
    ]
  },

  "requisitos_edad": {
    "_descripcion": "Requisitos de edad al final del titulo",
    "_nota": "FASE 1 v2.2 - detectados en revision. ORDEN IMPORTANTE: rangos antes que simples",
    "patrones": [
      {"patron": "\\s*\\d{2}\\s*[-a]\\s*\\d{2}\\s*a[ñn]os?\\s*$", "ejemplo": "25-35 años", "_nota": "PRIMERO: rango"},
      {"patron": "\\s*[Dd]e\\s*\\d{2}\\s*a\\s*\\d{2}\\s*a[ñn]os?\\s*$", "ejemplo": "de 25 a 35 años"},
      {"patron": "\\s*[Hh]asta\\s*\\d{2}\\s*a[ñn]os?\\s*$", "ejemplo": "hasta 30 años"},
      {"patron": "\\s*[Mm]ayores?\\s*(?:de\\s*)?\\d{2}\\s*a[ñn]os?\\s*$", "ejemplo": "mayores de 45 años"},
      {"patron": "\\s*[Mm]enores?\\s*(?:de\\s*)?\\d{2}\\s*a[ñn]os?\\s*$", "ejemplo": "menores de 30 años"},
      {"patron": "\\s*\\+\\s*\\d{2}\\s*a[ñn]os?\\s*$", "ejemplo": "+45 años", "_nota": "ULTIMO: simple con +"}
    ]
  },

