{
  "_version": "2.2",
  "_descripcion": "Patrones para limpiar titulos de ofertas - eliminar ruido geografico/empresarial/administrativo",
  "_fecha": "2026-01-13",
  "_cambios_v22": "Agregados: codigos_final, modalidad_guion, requisitos_edad, más patrones ubicacion",

  "codigos_inicio": {
    "_descripcion": "Codigos alfanumericos al INICIO del titulo (ej: 671SI, ABC123)",
    "patrones": [
      {"patron": "^\\d{2,6}[A-Z]{1,3}\\s+", "ejemplo": "671SI Operarios..."},
      {"patron": "^[A-Z]{1,3}\\d{2,6}\\s+", "ejemplo": "SI671 Operarios..."},
      {"patron": "^\\d{3,6}\\s+", "ejemplo": "12345 Vendedor..."},
      {"patron": "^[A-Z]{2,4}-?\\d{3,6}\\s+", "ejemplo": "REF-12345 Analista..."}
    ]
  },

  "prefijos_genericos": {
    "_descripcion": "Prefijos genericos al inicio que no aportan informacion (Busqueda Laboral:, etc)",
    "patrones": [
      {"patron": "^[Bb][úu]squeda\\s+[Ll]aboral\\s*[:\\-–]\\s*", "ejemplo": "Búsqueda Laboral: Modelista..."},
      {"patron": "^[Bb][úu]squeda\\s*[:\\-–]\\s*", "ejemplo": "Búsqueda: Analista..."},
      {"patron": "^[Ss]e\\s+[Bb]usca\\s*[:\\-–]?\\s*", "ejemplo": "Se busca: Vendedor..."},
      {"patron": "^[Bb]uscamos\\s*[:\\-–]?\\s*", "ejemplo": "Buscamos: Desarrollador..."},
      {"patron": "^[Oo]portunidad\\s*[:\\-–]\\s*", "ejemplo": "Oportunidad: Contador..."},
      {"patron": "^[Vv]acante\\s*[:\\-–]\\s*", "ejemplo": "Vacante: Recepcionista..."},
      {"patron": "^[Oo]ferta\\s+[Ll]aboral\\s*[:\\-–]\\s*", "ejemplo": "Oferta Laboral: Cajero..."},
      {"patron": "^[A-ZÁÉÍÓÚ][A-ZÁÉÍÓÚ\\s]+\\s+[Bb][Uu][Ss][Cc][Aa]\\s+", "ejemplo": "CAFETERIA FAST FOOD BUSCA Cajeros...", "_nota": "EMPRESA EN MAYUS + BUSCA"},
      {"patron": "^[A-Za-záéíóú]+\\s+(?:de\\s+)?[A-Za-záéíóú]+\\s+[Bb]usca\\s+(?:incorporar\\s+)?", "ejemplo": "Estudio de arquitectura busca dibujante...", "_nota": "Empresa en minusc + busca"}
    ]
  },

  "info_administrativa": {
    "_descripcion": "Informacion administrativa (fechas, horarios, sucursales)",
    "patrones": [
      {"patron": "\\s*-?\\s*pres\\.?\\s+\\d{1,2}/\\d{1,2}.*$", "ejemplo": "- pres 31/10 de 10 a 1130..."},
      {"patron": "\\s*-?\\s*presentarse\\s+.*$", "ejemplo": "- presentarse lunes..."},
      {"patron": "\\s*-?\\s*NUEVA\\s+Suc\\.?.*$", "ejemplo": "- NUEVA Suc. SAN ISIDRO"},
      {"patron": "\\s*-?\\s*Suc\\.?\\s+[A-Z][A-Za-z\\s]+$", "ejemplo": "- Suc. Palermo"},
      {"patron": "\\s*-?\\s*Sucursal\\s+[A-Za-z\\s]+$", "ejemplo": "- Sucursal Centro"}
    ]
  },

  "abreviaturas_expandir": {
    "_descripcion": "Abreviaturas a expandir o limpiar",
    "patrones": [
      {"patron": "\\bc/\\s*Tit\\.?\\s*secundario\\b", "reemplazo": "", "ejemplo": "c/ Tit. secundario"},
      {"patron": "\\bc/\\s*exp\\.?\\b", "reemplazo": "con experiencia", "ejemplo": "c/ exp."},
      {"patron": "\\bind\\.\\s+", "reemplazo": "industria ", "ejemplo": "ind. alimenticia"},
      {"patron": "\\bAlim\\.\\b", "reemplazo": "Alimenticia", "ejemplo": "Alim."}
    ]
  },

  "ciudades_inicio": {
    "_descripcion": "Ciudades que aparecen AL INICIO del titulo y deben eliminarse",
    "lista": [
      "Venado Tuerto", "Mar del Plata", "San Miguel", "San Isidro",
      "La Plata", "Villa Maria", "Rio Cuarto", "San Juan",
      "Bahia Blanca", "San Fernando", "Vicente Lopez", "Pilar",
      "Tigre", "Quilmes", "Lomas de Zamora", "Avellaneda",
      "San Martin", "Tres de Febrero", "Moron", "Merlo"
    ]
  },

  "codigos_referencia": {
    "_descripcion": "Patrones de codigos internos a eliminar",
    "patrones": [
      {"patron": "\\(req\\d+\\)", "ejemplo": "(req199380)"},
      {"patron": "\\[REF[^\\]]+\\]", "ejemplo": "[REF-123]"},
      {"patron": "#\\d{4,}", "ejemplo": "#12345"}
    ]
  },

  "modalidad_final": {
    "_descripcion": "Palabras de modalidad al final del titulo",
    "palabras": ["Eventual", "Part-time", "Part time", "Full-time", "Full time",
                 "Remoto", "Hibrido", "Hibrida", "Presencial", "Temporal", "Freelance"]
  },

  "zonas_ubicaciones": {
    "_descripcion": "Patrones de ubicacion al final - ORDEN IMPORTANTE: especificos primero",
    "patrones": [
      {"patron": "\\s*[Zz]/[\\w]+$", "ejemplo": "Z/Escobar"},
      {"patron": "\\s*[Zz]ona\\s+[\\w/]+$", "ejemplo": "zona CABA"},
      {"patron": "\\s*-\\s*[\\w\\s]+\\s*-\\s*(BA|GBA|CABA)$", "ejemplo": "- Roque Perez - BA", "_nota": "PRIMERO: patron especifico Localidad - Prov"},
      {"patron": "\\s*-\\s*GBA\\s+(Norte|Sur|Oeste)$", "ejemplo": "- GBA Norte"},
      {"patron": "\\s*-\\s*Zona\\s+[A-Z][a-záéíóúü]+(?:\\s+[A-Z]?[a-záéíóúü]+)*$", "ejemplo": "- Zona Rosario"},
      {"patron": "\\s+Zona\\s+(Norte|Sur|Oeste|Este)$", "ejemplo": "Zona Norte"},
      {"patron": "\\s*-\\s*(PRESENCIAL|REMOTO|HIBRIDO|HÍBRIDO)\\s+(CABA|GBA|BA|Capital|Buenos\\s*Aires)$", "ejemplo": "- PRESENCIAL CABA", "_nota": "Modalidad + ubicacion"},
      {"patron": "\\s*-\\s*(BA|CABA|GBA|Bs\\.?\\s*As\\.?|Buenos\\s+Aires|Capital|Cordoba|Córdoba|Mendoza|Rosario|Santa\\s+Fe)$", "ejemplo": "- Buenos Aires", "_nota": "ULTIMO: patron simple provincia"},
      {"patron": "[,.]?\\s*[Zz]ona\\s+[A-Za-záéíóú]+(?:\\s+[oy]\\s+alrededores)?[.!]?.*$", "ejemplo": ", zona Benavidez o alrededores. Leer requisitos", "_nota": "zona X o alrededores + texto extra"}
    ]
  },

