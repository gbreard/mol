      {"patron": "\\s*-\\s*[Rr]osario$", "ejemplo": "- Rosario"},
      {"patron": "\\s*-\\s*[Cc][óo]rdoba$", "ejemplo": "- Córdoba"},
      {"patron": "\\s*-\\s*[Mm]endoza$", "ejemplo": "- Mendoza"},
      {"patron": "\\s*-\\s*[Pp]alermo$", "ejemplo": "- Palermo"},
      {"patron": "\\s*-\\s*[Mm]icrocentro$", "ejemplo": "- Microcentro"},
      {"patron": "\\s*-\\s*[Pp]uerto\\s*[Mm]adero$", "ejemplo": "- Puerto Madero"},
      {"patron": "\\s*-\\s*[Cc]entro$", "ejemplo": "- Centro"},
      {"patron": "\\s*-\\s*[Cc]aballito$", "ejemplo": "- Caballito"},
      {"patron": "\\s*-\\s*[Bb]elgrano$", "ejemplo": "- Belgrano"},
      {"patron": "\\s*-\\s*[Nn]u[ñn]ez$", "ejemplo": "- Nuñez"},
      {"patron": "\\s*-\\s*[Rr]ecoleta$", "ejemplo": "- Recoleta"},
      {"patron": "\\s*-\\s*CABA$", "ejemplo": "- CABA"},
      {"patron": "\\s*-\\s*GBA$", "ejemplo": "- GBA"},
      {"patron": "\\s*-\\s*Capital$", "ejemplo": "- Capital"},
      {"patron": "\\s*-\\s*Pcia\\.?\\s*Bs\\.?\\s*As\\.?$", "ejemplo": "- Pcia Bs As"}
    ]
  }
}
